{
  "name": "@pallas-ui/panda-preset",
  "version": "0.0.1",
  "description": "Panda CSS Preset for Pallas UI",
  "keywords": [
    "Pallas UI",
    "Panda CSS",
    "Preset",
    "Pallas UI"
  ],
  "homepage": "https://pallas-ui.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "lint": "biome lint ./src",
    "typecheck": "tsc",
    "test": "tsx ./src/utils/create-color-tokens.test.ts"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.1.0",
    "@pallas-ui/typescript-config": "workspace:*",
    "effect": "3.11.3"
  },
  "devDependencies": {
    "@pandacss/dev": "0.48.0",
    "@pandacss/types": "^0.49.0",
    "@types/node": "^20",
    "clean-package": "2.2.0",
    "tsup": "8.3.5",
    "tsx": "^4.19.2"
  },
  "peerDependencies": {
    "@pandacss/dev": ">0.22.0"
  },
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./src/utils/index.ts",
        "default": "./src/utils/index.ts"
      },
      "require": {
        "types": "./dist/utils/index.d.cts",
        "default": "./dist/utils/index.cjs"
      }
    },
    "./colors/*": {
      "import": {
        "types": "./src/colors/*.ts",
        "default": "./src/colors/*.ts"
      },
      "require": {
        "types": "./dist/colors/*.d.cts",
        "default": "./dist/colors/*.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./src/types/index.ts",
        "default": "./src/types/index.ts"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      }
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/colors/*.ts",
      "src/utils/index.ts"
    ],
    "clean": true,
    "dts": true,
    "sourcemap": true,
    "format": [
      "esm",
      "cjs"
    ]
  }
}